{
    "name": "dev",
    "image": "duckietown-irl:latest",
    "containerUser": "root",
    "containerEnv": {
        "WANDB_API_KEY": "b745122a56e24173cf208c17d1b5d1b71f8a2967",
        "WANDB_ENTITY": "real-lab",
        "WANDB_PROJECT": "duckietown-irl"
    },
    "features": {
        "ghcr.io/devcontainers/features/docker-outside-of-docker": {
            "version": "latest",
            "enableNonRootDocker": "true",
            "moby": "true",
            "tty": "true"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.debugpy"
            ]
        }
    },
    "runArgs": [
        "--gpus",
        "all",
        "--tty",
        "--init",
        "--shm-size",
        "8G"
    ]
}